  <node>
    <group>Example</group>
    <start_before>LatencyDemoClient</start_before>
    <working_directory>$AMW_ACTIVE_ENVIRONMENT_ROOT</working_directory>
    <environment>LD_LIBRARY_PATH, AMW_SERVICE_CENTER_URL</environment>
    <command>LD_LIBRARY_PATH=$LD_LIBRARY_PATH $AMW_ACTIVE_ENVIRONMENT_ROOT/LatencyDemo 0 --alias=LatencyDemoServer</command>
    <runasroot>1</runasroot>
    <startup_time>0s</startup_time>
    <name>LatencyDemoServer</name>
  </node>
